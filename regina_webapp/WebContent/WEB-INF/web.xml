<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
  <display-name>regina_webapp_new</display-name>
  <context-param>
    <param-name>db.url</param-name>
    <!-- param-value>jdbc:h2:mem:sb</param-value-->
    <param-value>jdbc:h2:${catalina.home}/conf/reginadb/dbfileNew</param-value>
    <!-- param-value>jdbc:h2:tcp://localhost/${catalina.home}/conf/reginadb/dbfileNew</param-value-->
  </context-param>
  <context-param>
    <param-name>db.user</param-name>
    <param-value>sa</param-value>
  </context-param>
  <context-param>
    <param-name>db.password</param-name>
    <param-value></param-value>
  </context-param>
  <listener>
    <listener-class>org.h2.server.web.DbStarter</listener-class>
  </listener>
  <servlet>
    <servlet-name>log4j-init</servlet-name>
    <servlet-class>it.sbsoft.servlets.Log4jInit</servlet-class>
    <init-param>
      <param-name>log4j-init-file</param-name>
      <param-value>${catalina.home}/conf/log4j_regina.properties</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>H2Console</servlet-name>
    <servlet-class>org.h2.server.web.WebServlet</servlet-class>
    <init-param>
      <param-name>webAllowOthers</param-name>
      <param-value></param-value>
    </init-param>
    <init-param>
      <param-name>trace</param-name>
      <param-value></param-value>
    </init-param>
    <load-on-startup>2</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>H2Console</servlet-name>
    <url-pattern>/console/*</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description></description>
    <display-name>DoctorInfoSrvlt</display-name>
    <servlet-name>DoctorInfoSrvlt</servlet-name>
    <servlet-class>it.sbsoft.servlets.DoctorInfoSrvlt</servlet-class>
    <load-on-startup>3</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>DoctorInfoSrvlt</servlet-name>
    <url-pattern>/DoctorInfoSrvlt</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>StaticInfoSrvlt</display-name>
    <servlet-name>StaticInfoSrvlt</servlet-name>
    <servlet-class>it.sbsoft.servlets.StaticInfoSrvlt</servlet-class>
    <load-on-startup>3</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>StaticInfoSrvlt</servlet-name>
    <url-pattern>/StaticInfoSrvlt</url-pattern>
  </servlet-mapping>
  <!-- servlet>
    <display-name>FloorList</display-name>
    <servlet-name>FloorList</servlet-name>
    <servlet-class>it.sbsoft.servlets.FloorList</servlet-class>
    <init-param>
      <param-name>persistProperties</param-name>
      <param-value>false</param-value>
    </init-param>
    <init-param>
      <param-name>simulatorMode</param-name>
      <param-value>false</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>FloorList</servlet-name>
    <url-pattern>/FloorList</url-pattern>
  </servlet-mapping-->
  <servlet>
    <display-name>FloorOccupancy</display-name>
    <servlet-name>FloorOccupancy</servlet-name>
    <servlet-class>it.sbsoft.servlets.FloorOccupancy</servlet-class>
    <init-param>
      <param-name>simulatorMode</param-name>
      <param-value>false</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>FloorOccupancy</servlet-name>
    <url-pattern>/FloorOccupancy</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>MaintSrvlt</display-name>
    <servlet-name>MaintSrvlt</servlet-name>
    <servlet-class>it.sbsoft.servlets.MaintSrvlt</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MaintSrvlt</servlet-name>
    <url-pattern>/MaintSrvlt</url-pattern>
  </servlet-mapping>

  <servlet>
    <display-name>CleaningSrvlt</display-name>
    <servlet-name>CleaningSrvlt</servlet-name>
    <servlet-class>it.sbsoft.servlets.CleaningSrvlt</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CleaningSrvlt</servlet-name>
    <url-pattern>/CleaningSrvlt</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>Home.htm</welcome-file>
  </welcome-file-list>
</web-app>